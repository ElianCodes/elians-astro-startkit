  "version": "0.0.1",
  "description": "An opinionated Astro starter kit by Elian",
  "packageManager": "pnpm@8.14.3",
  "engines": {
    "node": "22"
  },
  "homepage": "https://github.com/ElianCodes/elians-astro-startkit",
  "repository": {
    "url": "git+https://github.com/ElianCodes/elians-astro-startkit",
    "type": "git"
  },
